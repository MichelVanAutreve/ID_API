{
  "name": "spacex",
  "version": "1.0.0",
  "description": "Interaction Design API Assignment",
  "private": true,
  "scripts": {
    "clean": "rimraf dist",
    "compile": "node-sass --output-style=expanded --source-map=true scss/screen.scss dist/screen.css",
    "prefix": "postcss dist/screen.css -u postcss-prefixer -o dist/screen.css",
    "minify": "cleancss --level=1 --source-map --source-map-inline-sources --output dist/screen.min.css dist/screen.css",
    "dev": "npm run compile -- --watch",
    "build": "npm run clean && npm run compile && npm run prefix && npm run minify && webpack"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/MichelVanAutreve/ID_API.git"
  },
  "keywords": [
    "Globe",
    "Interaction",
    "Design",
    "Web",
    "Development",
    "JavaScript",
    "CSS",
    "HTML",
    "Node.js",
    "npm",
    "WebPack",
    "Micro",
    "Interactions"
  ],
  "author": "Michel Van Autr√®ve",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/MichelVanAutreve/ID_API/issues"
  },
  "homepage": "https://github.com/MichelVanAutreve/ID_API#readme",
  "devDependencies": {
    "clean-css-cli": "^5.4.1",
    "node-sass": "^6.0.1",
    "postcss": "^8.3.9",
    "postcss-cli": "^9.0.1",
    "postcss-prefixer": "^2.1.3",
    "webpack": "^5.58.2",
    "webpack-cli": "^4.9.0"
  }
}
